{
  "name": "cursor-intellij-bridge",
  "displayName": "Cursor到IntelliJ跳转",
  "description": "从Cursor跳转到IntelliJ IDEA并打开当前文件",
  "version": "1.0.0",
  "publisher": "YourName",
  "repository": {
    "type": "git",
    "url": "https://github.com/superDoubling/cursor-to-idea.git"
  },
  "icon": "./images/icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "cursor",
    "intellij",
    "idea",
    "bridge",
    "editor"
  ],
  "activationEvents": [
    "onCommand:cursorIntellijBridge.jumpToIntelliJ"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursorIntellijBridge.jumpToIntelliJ",
        "title": "跳转到IntelliJ IDEA"
      }
    ],
    "keybindings": [
      {
        "command": "cursorIntellijBridge.jumpToIntelliJ",
        "key": "cmd+y",
        "mac": "cmd+y",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Cursor到IntelliJ跳转",
      "properties": {
        "cursorIntellijBridge.ideaPath": {
          "type": "string",
          "default": "",
          "description": "IntelliJ IDEA应用程序的自定义路径"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "@vscode/vsce": "^2.32.0",
    "typescript": "^4.4.4"
  }
}
